<article class="post clearfix">
  <header>
    <section id="navigation"></section>
    <h1>archives</h1>
  </header>
  <section class="body">
    <div class="inner">
      <ul>
      <% site.posts.slice().reverse().forEach(function(post){ %>
        <li>
          <a href="/<%= post.htmlPath() %>"><%= post.title() %></a>
        </li>
      <% }) %>
      </ul>
    </div>
  </section>
</article>



